server.port=8070
spring.application.name=consumerService
eureka.client.service-url.defaultZone=http://localhost:8764/eureka/


hystrix.command.default.execution.isolation.thread.timeoutInMilliseconds=3000
hystrix.command.consumer.cirecuitBreaker.enabled=true
hystrix.command.consumer.cirecuitBreaker.requestVolumeThreshold=10
hystrix.command.consumer.cirecuitBreaker.sleepWindowMilliseconds=10000
hystrix.command.consumer.cirecuitBreaker.errorThresholdPercetage=60
